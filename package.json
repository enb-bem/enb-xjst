  "version" : "1.0.4",
  "repository": "git://github.com/enb-make/enb-xjst",
  "homepage": "https://github.com/enb-make/enb-xjst",
  "bugs": "https://github.com/enb-make/enb-xjst/issues",
  "licenses": [{

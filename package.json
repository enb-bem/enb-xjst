  }],
  "contributors": [
    {
      "name": "Marat Dulin",
      "email": "mdevils@yandex.ru"
    },
    {
      "name": "Andrew Abramov",
      "email": "andrewblond@yandex.com"
    }
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "peerDependencies": {
    "enb": ">= 0.8.22 < 1.0.0"
  },
  "dependencies" : {
    "sibling": "0.1.3",
    "vow": "0.4.3",
    "pinpoint": "1.1.0",
    "bem-bl": "git://github.com/bem/bem-bl.git#v1.3.1"
  },
  "devDependencies" : {
    "enb": ">= 0.8.22 < 1.0.0",
    "bower": "1.3.3",
    "enb-validate-code": "0.0.1",
    "mocha": "1.18.2",
    "must": "0.11.0",
    "jshint": "2.5.1",
    "jscs": "1.4.5"
  },
  "scripts": {
    "test": "npm run lint && npm run func-test",
    "func-test": "npm run clean && npm run build && mocha test/func",
    "lint": "jshint . && jscs -c .jscs.js .",
    "build": "npm run deps && cd examples/bemhtml && enb make --no-cache && cd ../bemtree && enb make --no-cache",
    "clean": "npm run deps && cd examples/bemhtml && enb make clean && cd ../bemtree && enb make clean",
    "deps": "cd examples && bower install"
  }
}
